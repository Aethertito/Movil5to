@model dynamic

@{
    ViewData["Title"] = "Profile";
}

<h2>Profile</h2>

<div>
    <p><strong>Name:</strong> @Model.Name</p>
    <p><strong>Email:</strong> @Model.Email</p>
    <p><strong>Role:</strong> @Model.Role</p>
</div>

<a href="@Url.Action("Logout", "Account")" class="btn btn-danger">Logout</a>
